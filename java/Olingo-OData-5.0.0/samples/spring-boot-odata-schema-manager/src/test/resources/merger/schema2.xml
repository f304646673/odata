<?xml version="1.0" encoding="UTF-8"?>
<edmx:Edmx xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx" Version="4.0">
  <edmx:DataServices>
    <Schema xmlns="http://docs.oasis-open.org/odata/ns/edm" Namespace="TestNamespace" Alias="TM">
      
      <!-- Entity Types -->
      <EntityType Name="Order">
        <Key>
          <PropertyRef Name="ID"/>
        </Key>
        <Property Name="ID" Type="Edm.Int32" Nullable="false"/>
        <Property Name="OrderDate" Type="Edm.DateTimeOffset"/>
        <Property Name="Status" Type="TestNamespace.OrderStatus"/>
        <Property Name="CustomerID" Type="Edm.Int32"/>
      </EntityType>
      
      <!-- Complex Types -->
      <ComplexType Name="ContactInfo">
        <Property Name="Phone" Type="Edm.String" MaxLength="20"/>
        <Property Name="Email" Type="Edm.String" MaxLength="200"/>
        <Property Name="Address" Type="TestNamespace.Address"/>
      </ComplexType>
      
      <!-- Enum Types -->
      <EnumType Name="ProductCategory">
        <Member Name="Electronics" Value="0"/>
        <Member Name="Clothing" Value="1"/>
        <Member Name="Books" Value="2"/>
        <Member Name="Other" Value="3"/>
      </EnumType>
      
      <!-- Functions -->
      <Function Name="GetOrdersByCustomer">
        <Parameter Name="customerId" Type="Edm.Int32"/>
        <ReturnType Type="Collection(TestNamespace.Order)"/>
      </Function>
      
      <!-- Actions -->
      <Action Name="CancelOrder">
        <Parameter Name="orderId" Type="Edm.Int32"/>
      </Action>
      
      <!-- Terms -->
      <Term Name="Description" Type="Edm.String"/>
      
      <!-- Type Definitions -->
      <TypeDefinition Name="OrderId" UnderlyingType="Edm.Int32"/>
      
      <!-- Entity Container -->
      <EntityContainer Name="DefaultContainer">
        <EntitySet Name="Orders" EntityType="TestNamespace.Order"/>
        <FunctionImport Name="GetOrdersByCustomer" Function="TestNamespace.GetOrdersByCustomer"/>
        <ActionImport Name="CancelOrder" Action="TestNamespace.CancelOrder"/>
      </EntityContainer>
      
    </Schema>
  </edmx:DataServices>
</edmx:Edmx>
