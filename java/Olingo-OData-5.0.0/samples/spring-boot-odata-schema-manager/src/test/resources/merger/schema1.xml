<?xml version="1.0" encoding="UTF-8"?>
<edmx:Edmx xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx" Version="4.0">
  <edmx:DataServices>
    <Schema xmlns="http://docs.oasis-open.org/odata/ns/edm" Namespace="TestNamespace" Alias="TM">
      
      <!-- Entity Types -->
      <EntityType Name="Customer">
        <Key>
          <PropertyRef Name="ID"/>
        </Key>
        <Property Name="ID" Type="Edm.Int32" Nullable="false"/>
        <Property Name="Name" Type="Edm.String" MaxLength="100"/>
        <Property Name="Email" Type="Edm.String" MaxLength="200"/>
      </EntityType>
      
      <EntityType Name="Product">
        <Key>
          <PropertyRef Name="ID"/>
        </Key>
        <Property Name="ID" Type="Edm.Int32" Nullable="false"/>
        <Property Name="Name" Type="Edm.String" MaxLength="100"/>
        <Property Name="Price" Type="Edm.Decimal" Precision="10" Scale="2"/>
      </EntityType>
      
      <!-- Complex Types -->
      <ComplexType Name="Address">
        <Property Name="Street" Type="Edm.String" MaxLength="100"/>
        <Property Name="City" Type="Edm.String" MaxLength="50"/>
        <Property Name="PostalCode" Type="Edm.String" MaxLength="20"/>
      </ComplexType>
      
      <!-- Enum Types -->
      <EnumType Name="OrderStatus">
        <Member Name="Pending" Value="0"/>
        <Member Name="Processing" Value="1"/>
        <Member Name="Shipped" Value="2"/>
        <Member Name="Delivered" Value="3"/>
      </EnumType>
      
      <!-- Functions -->
      <Function Name="GetCustomerByEmail">
        <Parameter Name="email" Type="Edm.String"/>
        <ReturnType Type="TestNamespace.Customer"/>
      </Function>
      
      <!-- Actions -->
      <Action Name="UpdateCustomer">
        <Parameter Name="customer" Type="TestNamespace.Customer"/>
      </Action>
      
      <!-- Terms -->
      <Term Name="DisplayName" Type="Edm.String"/>
      
      <!-- Type Definitions -->
      <TypeDefinition Name="CustomerId" UnderlyingType="Edm.Int32"/>
      
      <!-- Entity Container -->
      <EntityContainer Name="DefaultContainer">
        <EntitySet Name="Customers" EntityType="TestNamespace.Customer"/>
        <EntitySet Name="Products" EntityType="TestNamespace.Product"/>
        <FunctionImport Name="GetCustomerByEmail" Function="TestNamespace.GetCustomerByEmail"/>
        <ActionImport Name="UpdateCustomer" Action="TestNamespace.UpdateCustomer"/>
      </EntityContainer>
      
    </Schema>
  </edmx:DataServices>
</edmx:Edmx>
