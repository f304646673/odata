<?xml version="1.0" encoding="UTF-8"?>
<edmx:Edmx Version="4.0" xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx">
  <edmx:Reference Uri="schemas/crossdir/dirA/common.xml">
    <edmx:Include Namespace="Test.DirA.Common"/>
  </edmx:Reference>
  <edmx:Reference Uri="schemas/crossdir/dirB/common.xml">
    <edmx:Include Namespace="Test.DirB.Common"/>
  </edmx:Reference>
  <edmx:DataServices>
    <Schema Namespace="Test.CrossRef.Integration" xmlns="http://docs.oasis-open.org/odata/ns/edm">
      <EntityType Name="Employee">
        <Key>
          <PropertyRef Name="ID"/>
        </Key>
        <Property Name="ID" Type="Edm.Int32" Nullable="false"/>
        <Property Name="PersonalInfo" Type="Test.DirA.Common.Address"/>
        <Property Name="CompanyContact" Type="Test.DirB.Common.ContactInfo"/>
      </EntityType>
      
      <EntityContainer Name="IntegrationContainer">
        <EntitySet Name="Employees" EntityType="Test.CrossRef.Integration.Employee"/>
      </EntityContainer>
    </Schema>
  </edmx:DataServices>
</edmx:Edmx>
