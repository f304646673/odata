      <TypeDefinition Name="EmailAddress" UnderlyingType="Edm.String"/>
      
      <EntityContainer Name="CoreContainer">
        <EntitySet Name="BaseEntities" EntityType="Core.Types.BaseEntity"/>
      </EntityContainer>
    </Schema>
  </edmx:DataServices>
