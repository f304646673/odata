<?xml version="1.0" encoding="UTF-8"?>
<!-- CircularC.xml - 循环依赖C，引用回A，形成A->B->C->A的循环 -->
<edmx:Edmx Version="4.0" xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx">
  <edmx:DataServices>
    <Schema Namespace="Test.CircularC" xmlns="http://docs.oasis-open.org/odata/ns/edm">
      <EntityType Name="EntityC">
        <Key>
          <PropertyRef Name="ID"/>
        </Key>
        <Property Name="ID" Type="Edm.Int32" Nullable="false"/>
        <!-- 引用CircularA的类型，完成循环依赖 -->
        <Property Name="RelatedA" Type="Test.CircularA.EntityA"/>
      </EntityType>
    </Schema>
  </edmx:DataServices>
</edmx:Edmx>
