<?xml version="1.0" encoding="UTF-8"?>
<edmx:Edmx xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx" Version="4.0">
    <edmx:DataServices>
        <Schema xmlns="http://docs.oasis-open.org/odata/ns/edm" Namespace="ConflictNamespace">
            <ComplexType Name="Address">
                <Property Name="Street" Type="Edm.String" MaxLength="100"/>
                <Property Name="City" Type="Edm.String" MaxLength="50"/>
                <Property Name="Country" Type="Edm.String" MaxLength="50"/>
            </ComplexType>
            
            <EntityType Name="Customer">
                <Key>
                    <PropertyRef Name="ID"/>
                </Key>
                <Property Name="ID" Type="Edm.Int32" Nullable="false"/>
                <Property Name="Name" Type="Edm.String" MaxLength="100"/>
                <Property Name="Address" Type="ConflictNamespace.Address"/>
            </EntityType>
            
            <EntityContainer Name="Container1">
                <EntitySet Name="Customers" EntityType="ConflictNamespace.Customer"/>
            </EntityContainer>
        </Schema>
    </edmx:DataServices>
</edmx:Edmx>
