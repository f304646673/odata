<?xml version="1.0" encoding="UTF-8"?>
<edmx:Edmx xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx" Version="4.0">
    <edmx:DataServices>
        <Schema xmlns="http://docs.oasis-open.org/odata/ns/edm" Namespace="ConflictNamespace">
            <EntityType Name="Product">
                <Key>
                    <PropertyRef Name="ID"/>
                </Key>
                <Property Name="ID" Type="Edm.Int32" Nullable="false"/>
                <Property Name="Name" Type="Edm.String" MaxLength="100"/>
                <Property Name="Price" Type="Edm.Decimal" Precision="18" Scale="2"/>
            </EntityType>
            
            <Function Name="GetDiscountedPrice" IsBound="true">
                <Parameter Name="product" Type="ConflictNamespace.Product"/>
                <Parameter Name="discountPercent" Type="Edm.Double"/>
                <ReturnType Type="Edm.Decimal" Precision="18" Scale="2"/>
            </Function>
            
            <EntityContainer Name="Container1">
                <EntitySet Name="Products" EntityType="ConflictNamespace.Product"/>
            </EntityContainer>
        </Schema>
    </edmx:DataServices>
</edmx:Edmx>
