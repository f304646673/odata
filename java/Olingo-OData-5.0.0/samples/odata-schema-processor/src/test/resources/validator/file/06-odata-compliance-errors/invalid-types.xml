<?xml version="1.0" encoding="UTF-8"?>
<edmx:Edmx Version="4.0" xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx">
  <edmx:DataServices>
    <Schema Namespace="Test.InvalidTypes" xmlns="http://docs.oasis-open.org/odata/ns/edm">
      <EntityType Name="Product">
        <Key>
          <PropertyRef Name="ID"/>
        </Key>
        <Property Name="ID" Type="Edm.Int32" Nullable="false"/>
        
        <!-- 使用了不存在的EDM类型 -->
        <Property Name="InvalidField1" Type="Edm.InvalidType"/>
        <Property Name="InvalidField2" Type="Edm.UnknownType"/>
        <Property Name="InvalidField3" Type="NonExistent.CustomType"/>
        
        <!-- 错误的类型格式 -->
        <Property Name="BadFormat1" Type="Edm..String"/>
        <Property Name="BadFormat2" Type=".Edm.String"/>
        <Property Name="BadFormat3" Type="Edm.String."/>
        
        <!-- 拼写错误的类型 -->
        <Property Name="Typo1" Type="Edm.Strng"/>
        <Property Name="Typo2" Type="Edm.Int33"/>
        <Property Name="Typo3" Type="Edm.DataTime"/>
      </EntityType>
      
      <ComplexType Name="Address">
        <!-- 复杂类型中的无效类型 -->
        <Property Name="Street" Type="Edm.InvalidString"/>
        <Property Name="City" Type="Unknown.CityType"/>
      </ComplexType>
    </Schema>
  </edmx:DataServices>
</edmx:Edmx>
