<?xml version="1.0" encoding="UTF-8"?>
<edmx:Edmx Version="4.0" xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx">
  <edmx:DataServices>
    <Schema Namespace="Test.InvalidXmlChars" xmlns="http://docs.oasis-open.org/odata/ns/edm">
      <!-- 包含一些可能导致XML解析问题的字符 -->
      <EntityType Name="Product">
        <Key>
          <PropertyRef Name="ID"/>
        </Key>
        <Property Name="ID" Type="Edm.Int32" Nullable="false"/>
        
        <!-- 包含可能的控制字符（以转义形式表示） -->
        <Property Name="NameWithControlChar" Type="Edm.String" DefaultValue="Product&#x01;Name"/>
        <Property Name="NameWithBell" Type="Edm.String" DefaultValue="Alert&#x07;Sound"/>
        <Property Name="NameWithBackspace" Type="Edm.String" DefaultValue="Text&#x08;Backspace"/>
        
        <!-- 包含垂直制表符和换页符 -->
        <Property Name="NameWithVTab" Type="Edm.String" DefaultValue="Line1&#x0B;Line2"/>
        <Property Name="NameWithFormFeed" Type="Edm.String" DefaultValue="Page1&#x0C;Page2"/>
        
        <!-- 包含一些高Unicode代码点 -->
        <Property Name="NameWithHighUnicode" Type="Edm.String" DefaultValue="Test&#x1FFFE;Character"/>
        <Property Name="NameWithSurrogate" Type="Edm.String" DefaultValue="Emoji&#x1F600;Test"/>
        
        <!-- 包含字节顺序标记(BOM) -->
        <Property Name="NameWithBOM" Type="Edm.String" DefaultValue="&#xFEFF;BOM_Test"/>
        
        <!-- 包含不完整的XML实体 -->
        <Property Name="IncompleteEntity" Type="Edm.String" DefaultValue="Test &incomplete entity"/>
        
        <!-- 包含无效的数字字符引用 -->
        <Property Name="InvalidCharRef" Type="Edm.String" DefaultValue="Test &#99999999; character"/>
      </EntityType>
    </Schema>
  </edmx:DataServices>
</edmx:Edmx>
