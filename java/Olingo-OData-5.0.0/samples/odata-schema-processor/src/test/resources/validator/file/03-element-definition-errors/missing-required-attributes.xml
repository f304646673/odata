<?xml version="1.0" encoding="UTF-8"?>
<edmx:Edmx Version="4.0" xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx">
  <edmx:DataServices>
    <Schema Namespace="Test.MissingRequiredAttributes" xmlns="http://docs.oasis-open.org/odata/ns/edm">
      <!-- EntityType缺少Key -->
      <EntityType Name="ProductWithoutKey">
        <Property Name="ID" Type="Edm.Int32" Nullable="false"/>
        <Property Name="Name" Type="Edm.String" MaxLength="50"/>
      </EntityType>
      
      <!-- Property缺少Type属性 -->
      <EntityType Name="ProductWithInvalidProperty">
        <Key>
          <PropertyRef Name="ID"/>
        </Key>
        <Property Name="ID" Type="Edm.Int32" Nullable="false"/>
        <Property Name="Name" MaxLength="50"/>
      </EntityType>
      
      <!-- EnumType没有成员 -->
      <EnumType Name="EmptyEnum">
        <!-- 没有Member元素 -->
      </EnumType>
      
      <!-- Member缺少Value属性 -->
      <EnumType Name="InvalidMembers">
        <Member Name="Active"/>
        <Member Name="Inactive" Value="2"/>
      </EnumType>
    </Schema>
  </edmx:DataServices>
</edmx:Edmx>
