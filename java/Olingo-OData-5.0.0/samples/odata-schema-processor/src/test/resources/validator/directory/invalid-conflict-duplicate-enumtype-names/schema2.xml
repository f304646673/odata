<?xml version="1.0" encoding="UTF-8"?>
<edmx:Edmx xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx" Version="4.0">
    <edmx:DataServices>
        <Schema xmlns="http://docs.oasis-open.org/odata/ns/edm" Namespace="ConflictNamespace">
            <EnumType Name="Status">
                <Member Name="New" Value="1"/>
                <Member Name="Processing" Value="2"/>
                <Member Name="Completed" Value="3"/>
                <Member Name="Cancelled" Value="4"/>
            </EnumType>
            
            <EntityType Name="Task">
                <Key>
                    <PropertyRef Name="ID"/>
                </Key>
                <Property Name="ID" Type="Edm.Int32" Nullable="false"/>
                <Property Name="Title" Type="Edm.String" MaxLength="100"/>
                <Property Name="TaskStatus" Type="ConflictNamespace.Status"/>
            </EntityType>
            
            <EntityContainer Name="Container2">
                <EntitySet Name="Tasks" EntityType="ConflictNamespace.Task"/>
            </EntityContainer>
        </Schema>
    </edmx:DataServices>
</edmx:Edmx>
