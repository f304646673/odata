<?xml version="1.0" encoding="utf-8"?>
<edmx:Edmx Version="4.0" xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx">
  <edmx:DataServices>
    <Schema Namespace="TestService" xmlns="http://docs.oasis-open.org/odata/ns/edm">
      <EntityType Name="Product">
        <Key>
          <PropertyRef Name="ID"/>
        </Key>
        <Property Name="ID" Type="Edm.Int32" Nullable="false"/>
        <Property Name="Name" Type="Edm.String" Nullable="false"/>
      </EntityType>

      <!-- 多个引用相同的外部架构 -->
      <EntityContainer Name="Container">
        <EntitySet Name="Products" EntityType="TestService.Product"/>
      </EntityContainer>
    </Schema>
  </edmx:DataServices>

  <!-- 引用同一个外部文件多次以测试性能 -->
  <edmx:Reference Uri="conflicting-namespace-1.xml">
    <edmx:Include Namespace="External.Service1"/>
  </edmx:Reference>
  <edmx:Reference Uri="conflicting-namespace-2.xml">
    <edmx:Include Namespace="External.Service2"/>
  </edmx:Reference>
  <edmx:Reference Uri="conflicting-namespace-1.xml">
    <edmx:Include Namespace="External.Service1" Alias="Service1Alias"/>
  </edmx:Reference>
</edmx:Edmx>

