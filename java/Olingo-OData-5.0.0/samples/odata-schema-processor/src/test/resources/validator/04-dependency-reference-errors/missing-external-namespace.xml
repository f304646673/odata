<?xml version="1.0" encoding="UTF-8"?>
<edmx:Edmx Version="4.0" xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx">
  <edmx:DataServices>
    <Schema Namespace="Test.MissingDependency" xmlns="http://docs.oasis-open.org/odata/ns/edm">
      <!-- 引用不存在的外部命名空间 -->
      <EntityType Name="Order">
        <Key>
          <PropertyRef Name="OrderID"/>
        </Key>
        <Property Name="OrderID" Type="Edm.Int32" Nullable="false"/>
        <!-- 引用了不存在的命名空间 NonExistent.Types.Customer -->
        <Property Name="Customer" Type="NonExistent.Types.Customer"/>
        <Property Name="OrderDate" Type="Edm.DateTimeOffset"/>
      </EntityType>
      
      <!-- EntityType继承了不存在的基类 -->
      <EntityType Name="SpecialProduct" BaseType="MissingNamespace.BaseProduct">
        <Property Name="SpecialFeature" Type="Edm.String" MaxLength="100"/>
      </EntityType>
      
      <!-- ComplexType引用了不存在的类型 -->
      <ComplexType Name="OrderDetails">
        <Property Name="Quantity" Type="Edm.Int32"/>
        <Property Name="Product" Type="Missing.ProductInfo"/>
      </ComplexType>
    </Schema>
  </edmx:DataServices>
</edmx:Edmx>
