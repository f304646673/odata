<?xml version="1.0" encoding="UTF-8"?>
<edmx:Edmx Version="4.0" xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx">
  <edmx:DataServices>
    <Schema Namespace="Test.WrongVersion" xmlns="http://docs.oasis-open.org/odata/ns/edm">
      <!-- 版本兼容性问题 -->
      <EntityType Name="Product">
        <Key>
          <PropertyRef Name="ID"/>
        </Key>
        <Property Name="ID" Type="Edm.Int32" Nullable="false"/>
        <Property Name="Name" Type="Edm.String"/>
        
        <!-- 使用了可能不兼容版本的语法 -->
        <NavigationProperty Name="Category" Type="Test.WrongVersion.Category" 
                           ContainsTarget="true" 
                           Partner="Products"/>
      </EntityType>
      
      <!-- 使用了可能在某些版本中不支持的特性 -->
      <EntityType Name="Category" OpenType="true">
        <Key>
          <PropertyRef Name="ID"/>
        </Key>
        <Property Name="ID" Type="Edm.Int32" Nullable="false"/>
        <Property Name="Name" Type="Edm.String"/>
        
        <NavigationProperty Name="Products" Type="Collection(Test.WrongVersion.Product)" 
                           Partner="Category"/>
      </EntityType>
      
      <!-- 可能在不同版本中语法不同的元素 -->
      <EnumType Name="Status" UnderlyingType="Edm.Int32" IsFlags="true">
        <Member Name="Active" Value="1"/>
        <Member Name="Inactive" Value="2"/>
        <Member Name="Pending" Value="4"/>
      </EnumType>
      
      <!-- 版本特定的容器定义 -->
      <EntityContainer Name="TestContainer">
        <EntitySet Name="Products" EntityType="Test.WrongVersion.Product">
          <NavigationPropertyBinding Path="Category" Target="Categories"/>
        </EntitySet>
        <EntitySet Name="Categories" EntityType="Test.WrongVersion.Category">
          <NavigationPropertyBinding Path="Products" Target="Products"/>
        </EntitySet>
      </EntityContainer>
    </Schema>
  </edmx:DataServices>
</edmx:Edmx>
