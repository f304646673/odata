<!--
错误原因：同一 Schema 下有两个同名 Action（参数签名相同）。
应报错：Action 名称+参数签名必须唯一。
-->
<edmx:Edmx xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx" Version="4.0">
  <edmx:DataServices>
    <Schema xmlns="http://docs.oasis-open.org/odata/ns/edm" Namespace="Test">
      <Action Name="A1">
        <Parameter Name="p1" Type="Edm.String"/>
      </Action>
      <Action Name="A1">
        <Parameter Name="p1" Type="Edm.String"/>
      </Action>
    </Schema>
  </edmx:DataServices>
</edmx:Edmx>
