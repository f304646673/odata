<?xml version="1.0" encoding="UTF-8"?>
<edmx:Edmx xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx" Version="4.0">
  <edmx:DataServices>
    <Schema xmlns="http://docs.oasis-open.org/odata/ns/edm" Namespace="TestNamespace">
      
      <!-- Valid EntityType -->
      <EntityType Name="Person">
        <Key>
          <PropertyRef Name="ID"/>
        </Key>
        <Property Name="ID" Type="Edm.Int32" Nullable="false"/>
        <Property Name="Name" Type="Edm.String" Nullable="false"/>
      </EntityType>
      
      <!-- Invalid: ComplexType attempting to inherit from EntityType -->
      <ComplexType Name="Employee" BaseType="TestNamespace.Person">
        <Property Name="Department" Type="Edm.String" Nullable="false"/>
        <Property Name="Salary" Type="Edm.Decimal" Nullable="false"/>
      </ComplexType>
      
      <EntityContainer Name="TestContainer">
        <EntitySet Name="People" EntityType="TestNamespace.Person"/>
      </EntityContainer>
      
    </Schema>
  </edmx:DataServices>
</edmx:Edmx>
