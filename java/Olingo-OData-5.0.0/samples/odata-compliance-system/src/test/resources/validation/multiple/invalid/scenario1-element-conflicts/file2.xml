<?xml version="1.0" encoding="utf-8"?>
<edmx:Edmx Version="4.0" xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx">
  <edmx:DataServices>
    <!-- 同一命名空间，重复定义Customer EntityType - 这应该引发冲突 -->
    <Schema Namespace="Conflicted.Models" xmlns="http://docs.oasis-open.org/odata/ns/edm">
      <EntityType Name="Customer">
        <Key>
          <PropertyRef Name="ID"/>
        </Key>
        <Property Name="ID" Type="Edm.String" Nullable="false"/>
        <Property Name="Name" Type="Edm.String"/>
        <Property Name="Phone" Type="Edm.String"/>
      </EntityType>
      
      <!-- 同一命名空间，重复定义Address ComplexType - 这应该引发冲突 -->
      <ComplexType Name="Address">
        <Property Name="StreetAddress" Type="Edm.String"/>
        <Property Name="PostalCode" Type="Edm.String"/>
      </ComplexType>
      
      <EntityContainer Name="Container">
        <EntitySet Name="Customers" EntityType="Conflicted.Models.Customer"/>
      </EntityContainer>
    </Schema>
  </edmx:DataServices>
</edmx:Edmx>
